{
  "name": "simple-online-healthcare/php-coding-standards",
  "description": "A repository for our PHP Coding Standards Ruleset for PHP-CS-Fixer.",
  "type": "library",
  "license": "proprietary",
  "authors": [
    {
      "name": "Josh Murray",
      "email": "josh.murray@simpleonlinehealthcare.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "SimpleOnlineHealthcare\\CodingStandards\\": "src"
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.14"
  },
  "scripts": {
    "php-cs-fixer:dummy": "./vendor/bin/php-cs-fixer fix --dry-run",
    "php-cs-fixer:real": "./vendor/bin/php-cs-fixer fix"
  }
}
